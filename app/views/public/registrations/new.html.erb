<div class="container">
  <div class="text-center">
      <h2>新規登録画面</h2>
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= render "public/shared/error_messages", resource: resource %>
      <table class="table">
        <tr>
          <td>姓</td>
          <td><%= f.text_field :last_name %></td>
          <td>名</td>
          <td><%= f.text_field :first_name %></td>
        </tr>
        <tr>
          <td>セイ</td>
          <td><%= f.text_field :last_name_kana %></td>
          <td>メイ</td>
          <td><%= f.text_field :first_name_kana %></td>
        </tr>
        <tr>
          <td>郵便番号</td>
          <td><%= f.text_field :postcode %></td>
        </tr>
        <tr>
          <td>住所</td>
          <td><%= f.text_field :address %></td>
        </tr>
        <tr>
          <td>電話番号</td>
          <td><%= f.text_field :telephone_number %></td>
        </tr>
        <tr>
          <td>生年月日(例: 19950101)</td>
          <td><%= f.text_field :birthday %></td>
        </tr>
        <tr>
          <td>メールアドレス</td>
          <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
        </tr>
        <tr>
          <td>パスワード(6文字以上)</td>
          <td><%= f.password_field :password, autocomplete: "new-password" %></td>
        </tr>
        <tr>
          <td>パスワード(確認用)</td>
          <td><%= f.password_field :password_confirmation, autocomplete: "new-password" %></td>
        </tr>
        <tr>
          <td></td>
          <td></td>
          <td></td>
          <td><%= f.submit "登録する", class: "btn btn-info" %></td>
        </tr>
      </table>
        <% end %>
  </div>
</div>
